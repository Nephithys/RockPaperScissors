<Window x:Name="PvPWin1" x:Class="RockPaperScissors.PvPWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RockPaperScissors"
        mc:Ignorable="d"
        Title="PvPWin" Height="264.3" Width="395.3">
    <Grid>
        <TextBox x:Name="TheWinnerIsText" HorizontalAlignment="Left" Height="23" Margin="95,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="197" TextChanged="TheWinnerIsText_TextChanged" IsEnabled="False"/>
        <TextBox x:Name="TheLoserIsText" HorizontalAlignment="Left" Height="23" Margin="95,119,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="197" TextChanged="TheLoserIsText_TextChanged"/>
        <Button x:Name="button" Content="Main Menu" HorizontalAlignment="Left" Margin="255,181,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Window>
